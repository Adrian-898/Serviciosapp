{
	"cli": {
		"version": ">=14.4.1",
		"appVersionSource": "remote"
	},
	"build": {
		"preview": {
			"android": {
				"buildType": "apk"
			},
			"autoIncrement": true,
			"environment": "preview"
		},
		"development": {
			"android": {
				"buildType": "apk"
			},
			"developmentClient": true,
			"distribution": "internal",
			"environment": "development"
		},
		"production": {
			"environment": "production"
		}
	}
}
